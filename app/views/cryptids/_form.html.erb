<%= form_for @cryptid do |f| %>

<%= f.label  "Name:" %>
<%= f.text_field :name %><br>

<%= f.label  "Location:" %>
<%= f.text_field :location %><br>

<%= f.label  "Size:" %>
<%= f.text_field :size %><br>

<%= f.label  "Classification:" %>
<%= f.text_field :classification %><br>

<%= f.submit "Add Cryptid" %>

<% end %>