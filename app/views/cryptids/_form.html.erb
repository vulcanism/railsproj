<%= form_for @cryptid do |f| %>

<%= f.hidden_field :observation_id %>

<%= f.label  "Name:" %>
<%= f.text_field :name %>

<%= f.label  "Location:" %>
<%= f.text_field :location %>

<%= f.label  "Size:" %>
<%= f.text_field :size %>

<%= f.label  "Type:" %>
<%= f.text_field :type %>

<%= f.submit "Add Cryptid" %>

<% end %>